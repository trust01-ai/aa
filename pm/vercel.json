{
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/proxy" }
  ],
  "headers": [
    {
      "source": "/aadcdn/(.*)",
      "headers": [
        { "key": "Host", "value": "aadcdn.espeharete.top" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Forwarded-Proto", "value": "https" },
        { "key": "X-Forwarded-Host", "value": "aa-iido.vercel.app" }
      ]
    }
  ]
}
